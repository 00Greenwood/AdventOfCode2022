{
  "devDependencies": {
    "@tsconfig/node16": "^1.0.3",
    "@types/argparse": "^2.0.10",
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "eslint": "^8.28.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.4",
    "prettier": "^2.8.0",
    "typescript": "^4.9.3"
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "compile": "tsc",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "presolve": "npm run compile",
    "solve": "node .build/src/solve.js",
    "prepare": "husky install"
  },
  "dependencies": {
    "argparse": "^2.0.1"
  },
  "lint-staged": {
    "*.ts": "eslint --cache --fix",
    "*.{ts,json,md}": "prettier --write"
  }
}
